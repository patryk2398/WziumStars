@model int
<td style="width:150px;">
    <div class="btn-group" role="group">
        <a class="btn btn-primary text-white" asp-action="Edytuj" asp-route-Id="@Model">
            <i class="fas fa-edit"></i>
        </a>
        <a class="btn btn-danger text-white" asp-action="Usun" asp-route-Id="@Model">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>
</td>